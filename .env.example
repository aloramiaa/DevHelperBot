# Discord Bot Settings
DISCORD_TOKEN=
DISCORD_CLIENT_ID=
PREFIX=!

# Database Settings
MONGODB_URI=

# API Keys
# GitHub token is optional for higher rate limits, but not required for basic functionality
GITHUB_TOKEN=
STACKOVERFLOW_KEY=

# Feature Flags
# All features are enabled by default (can be controlled in config.js)
ENABLE_MARKDOWN_RENDERER=true
ENABLE_GITHUB_STATS=true
ENABLE_GOODFIRSTISSUE=true
ENABLE_REGEX_TOOLS=true
ENABLE_DEVTO_INTEGRATION=true
ENABLE_API_EXPLORER=true

# Service Settings
MAX_ARTICLES_PER_REQUEST=5
CACHE_DURATION=3600000  # 1 hour in milliseconds 